{
  "manifest_version": 2,
  "name": "No War Extension",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content_script.js"]
    }
  ],
  "permissions": ["<all_urls>", "storage", "webRequest"],
  "web_accessible_resources": ["assets/no_war.svg"]
}
